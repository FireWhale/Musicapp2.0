<h1>All Sources</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Activity</th>
    <th>Obtained</th>
  </tr>

<% @sourcessorted.each do |source| %>
  <tr>
    <td><%= link_to source.name, source %></td>
    <td><%= source.activity %></td>
    <td><%= source.obtained %></td>
  </tr>
<% end %>
</table>

